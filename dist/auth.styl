$plugin = 'uloc-vue-auth'
.app-auth-color {
  display: flex;
  flex-direction: column;
  justify-content: center;
  background: linear-gradient(to top right, rgba(255, 82, 99, 0.9) 10%, rgba(255, 115, 129, 0.9) 65%, rgba(252, 189, 1, 0.9) 125%);
  min-height: 100vh;
  a.logo{
    width 120px
    height 59px
    background url("./assets/logo.png") center no-repeat
    background-size contain
    overflow hidden
    text-indent -5000px
    display inline-block
  }
  .auth-header {
    flex-shrink: 0;
    margin: 0 0 30px -5px;
  }
  .auth-container {
    flex-shrink: 0;
    margin: 0 auto;
    width: 95%;
  }
  .auth-content{
    background-color: #fff;
    border-radius: 4px;
    box-shadow: 0 1px 3px 0 rgba(0,0,0,0.15);
    line-height: 1.5;
    text-align: center;
    .title{
      color: #151b26;
      font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Helvetica,Arial,sans-serif;
      font-size: 32px;
      font-weight: 400;
      line-height: 40px;
      padding: 45px 30px 30px;
      text-transform: none;
    }
    .btn-def{
      font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Helvetica,Arial,sans-serif; 
      font-weight: 400;
      background-color: #14aaf5 !important;
      border-radius: 4px;
      height: 40px;
      line-height: 40px;
      padding: 0 15px;
      font-size: 13px;
      &.login{
        /*background-color #26a69a !important*/
      }
    }
    .auth-separator {
      color: #b7bfc6;
      font-size: 24px;
      margin-top: 35px;
      position: relative;
      &:before, &:after {
        border-top: 1px solid #e0e6e8;
        content: "";
        height: 1px;
        left: 0;
        position: absolute;
        top: 50%;
        width: 45%;
        z-index: 0;
      }
      &:before {
        left: auto;
        right: 0;
      }
    }
    .form-input {
      margin-top: 30px;
      text-align: left;
      &.text-right{
        text-align: right;
      }
    }
    .form-input label {
      color: #848f99;
      font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Helvetica,Arial,sans-serif;
      font-size: 12px;
      font-weight: 500;
      display: block;
      margin-bottom: 7px;
    }
    .form-input input, .form-input select, .form-input textarea {
      background-color: #fff;
      border: 1px solid #d5dce0;
      border-radius: 4px;
      color: #151b26;
      height: 40px;
      margin: 0;
      padding: 0 0 0 10px;
      width: 100%;
    }
    .form-input input {
      line-height: normal;
    }
    .auth-form {
      padding: 0 30px;
    }
    form.auth-form {
      padding-bottom: 45px;
    }
    @media (min-width: 48em) {
      .auth-form {
        padding: 0 90px;
      }
    }
    .input--help {
      display: block;
      margin-top: 10px;
    }
    .input--help a {
      border-bottom: 1px dotted #848f99;
      color: #848f99;
      font-size: 11px;
      text-align: left;
      &:hover {
        border-bottom: 1px solid #848f99;
        text-decoration: none;
      }
    }
    .login-errors {
      padding: 10px;
      color: #900
    }
  }
  @media (min-width: 48em) {
    .auth-container {
      width: 585px;
    }
  }
  .auth-footer{
    color: #FFFFFF;
    text-align: center
    margin-top: 15px;
    font-size:14px;
    font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Helvetica,Arial,sans-serif; 
  }
}
.app-auth {
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  position: fixed;
  background-size: cover;
  background-color: #0F0F0F;
  z-index: 10000;
  a, button, input {
    outline: none !important;
  }
  .clearfloat {
    clear: both;
    height: 0;
    font-size: 1px;
    line-height: 0px;
  }
  .oculto {
    display: none
  }
  .img-block {
    display: inline-block !important
  }
  
  
  .background {
    background-repeat: no-repeat;
    
    background-position: 50% 50%;
    
    background-position: 50% 50% !important;
  }
  
  .fullscreen,
  .content-a {
    width: 100%;
    min-height: 100%;
  }
  .not-fullscreen,
  .not-fullscreen .content-a,
  .fullscreen.not-overflow,
  .fullscreen.not-overflow .content-a {
    height: 100%;
    overflow: hidden;
  }
  
  .content-a {
    display: table;
  }
  .content-b {
    display: table-cell;
    position: relative;
    vertical-align: middle;
    text-align: center;
  }
  
  .not-fullscreen {
    height: 50%;
  }
  
  html {
    height: 100%;
    font-weight: 300;
  }
  body {
    font-weight: 300;
    background-color: #0F0F0F
  }
  .content-overflow {
  }
  ul, li {
    padding: 0;
    margin: 0;
  }
  h1, h2, h3, h4 {
    font-weight: 300;
    color: #666666;
    padding: 0;
    margin: 0 0 16px 0;
  }
  h1 {
    font-size: 32px
  }
  h2 {
    font-size: 28px
  }
  h3 {
    font-size: 18px
  }
  h4 {
    font-size: 16px
  }
  h1 strong, h2 strong, h3 strong, h4 strong {
    font-weight: 400
}
  .center-full.cinza {
    background-color: #F6F7F7;
    padding: 50px 0;
  }
  .center-full.azul {
    background-color: #E4EEF2;
    padding: 50px 0;
  }
  .bg-white {
    background-color: #FFFFFF;
  }
  .content {
    clear: both;
  }
  .clearboth {
    clear: both
  }
  .bg-c {
    position: relative;
  }
  li img,
  .bg, .bg img {
    -webkit-transition: opacity 1.4s;
    -moz-transition: opacity 1.4s;
    -ms-transition: opacity 1.4s;
    transition: opacity 1.4s;
  }
  .bg {
    position: fixed;
    z-index: -1;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background-size: cover;
    opacity: 0.8;
  }
  .bg img {
    display: none;
  }
  .bg.is-loading img, .bg.is-loading {
    opacity: 0;
  }
  .container-login {
    padding: 25px 30px;
    z-index: 2;
  }
  .logo {
    width: 177px;
    height: 48px;
    background-repeat: no-repeat !important;
    background url("./assets/logo.png")
    background-size: contain !important;
    overflow: hidden;
    text-indent: -5000px;
    display: block;
  }
  .d-t {
    bottom: 0;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    display: table;
    height: 100%;
    width: 100%;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
  }
  .d-r {
    display: table-row;
    height: 100%;
  }
  .d-c {
    display: table-cell;
    height: 100%;
    vertical-align: middle;
    text-align: center;
  }
  .d-c.r {
    padding-right: 46px;
    width: 1px;
    text-align: left;
  }
  .timer {
    display: inline-block;
  }
  .timer h1 {
    font-size: 150px;
    text-transform: uppercase;
    color: #FFF;
    font-weight: 300;
    margin: 0;
    text-shadow: -1px -1px 0 rgba(0, 0, 0, .1), 1px -1px 0 rgba(0, 0, 0, .1), -1px 1px 0 rgba(0, 0, 0, .1), 1px 1px 0 rgba(0, 0, 0, .1);
  }
  .timer h2 {
    text-align: center;
    display: block;
    font-size: 30px;
    text-transform: uppercase;
    color: #FFF;
    text-shadow: -1px -1px 0 rgba(0, 0, 0, .1), 1px -1px 0 rgba(0, 0, 0, .1), -1px 1px 0 rgba(0, 0, 0, .1), 1px 1px 0 rgba(0, 0, 0, .1);
  }
  #login1 {
  }
  #login2 {
    display: none;
    background-color: #000;
    min-height: 200px;
  }
  .login-tab {
    width: 450px;
    background-color: #FFF;
    -webkit-border-radius: 8px;
    -moz-border-radius: 8px;
    border-radius: 8px;
    text-align: left !important;
    overflow: hidden;
  }
  .login-tab form {
    margin: 0;
    padding: 0 20px;
  }
  .login-tab .pcontent {
  }
  .login-tab ul.options {
    list-style: none;
    list-style-type: none;
    margin: 0;
    padding: 0;
  }
  .login-tab ul.options li a {
    font-size: 20px;
    float: left;
    width: 50%;
    display: block;
    font-weight: 300;
    color: #FFF;
    padding: 30px 20px 20px;
    margin: 0;
    background-color: #427FED;
    text-decoration: none !important;
  }
  .login-tab ul.options li a:hover {
    text-decoration: none;
    -webkit-filter: brightness(1.1);
    -moz-filter: brightness(1.1);
    -ms-filter: brightness(1.1);
    -o-filter: brightness(1.1);
    filter: brightness(1.1);
  }
  .login-tab ul.options li.selected a {
    color: #333;
    background-color: transparent;
  }
  .form-login {
    clear: both;
  }
  .form-login label {
    display: none;
    font-family: Arial, Helvetica, sans-serif;
  }
  .input-login {
    display: block;
    background: none;
    width: 100%;
    margin: 20px 0;
    border: none;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    padding: 10px;
    border-bottom: #ccc 1px solid;
    cursor: text;
    -webkit-transition: all 150ms linear, border-color 0s;
    transition: all 150ms linear, border-color 0s;
    font-size: 16px;
  }
  input:hover.input-login {
    background-color: #F8F8F8;
  }
  .s-i-l {
    width: 100%;
    display: block;
    margin: auto;
  }
  label[for="remember_me"], label[for="remember_me2"] {
    display: block;
    float: left;
    font-weight: normal;
    margin-bottom: 20px;
  }
  input.lembrar {
    float: left;
    margin-right: 6px;
    margin-bottom: 20px;
  }
  .hdiv {
    margin-bottom: 40px;
  }
  .btn-login {
    border: none;
    display: block;
    width: 100%;
    margin: auto;
    margin-bottom: 30px;
    padding: 10px;
    text-align: center;
    font-size: 20px;
    color: #FFF;
    font-weight: 400;
    background-color: #427FED;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
  }
  .login-errors {
    padding: 0 20px 0;
    color: #900
  }
  .login-tab.dev {
    background-color: #000;
  }
  .login-tab.dev ul.options li.selected a {
  }
  .login-tab.dev ul.options li.selected a {
    color: #FFF;
    background-color: #000;
  }
  .login-tab.dev .input-login {
    display: inline-block;
    background: none;
    width: 70%;
    margin: 0;
    margin-bottom: 0;
    float: left;
    border: none;
    -webkit-border-radius: 0px;
    -moz-border-radius: 0px;
    border-radius: 0px;
    padding: 5px;
    border-bottom: none;
    cursor: default;
    color: #FFF;
    font-size: 16px;
    font-family: Arial, Helvetica, sans-serif;
  }
  .login-tab.dev input:hover.input-login {
    background-color: transparent;
  }
  .login-tab.dev input.password {
    margin-bottom: 40px;
    display: none;
  }
  .login-tab.dev .btn-login {
    background: none;
    background-color: transparent;
    border: none;
    text-indent: -5000px;
    overflow: hidden;
    width: 1px;
    height: 1px;
  }
  .login-tab.dev label {
    float: left;
    clear: left;
    margin-left: 15px;
    display: none;
    color: #FFF;
    font-weight: normal;
    padding: 5px;
    margin-top: 0;
    margin-bottom: 0;
    font-size: 16px;
  }
  .login-tab.dev label[for="username2"] {
    display: block;
  }
  .login-tab.dev .username {
    margin-top: 0;
  }
  .text-bottom-left {
    text-shadow: -1px -1px 0 rgba(0, 0, 0, .1), 1px -1px 0 rgba(0, 0, 0, .1), -1px 1px 0 rgba(0, 0, 0, .1), 1px 1px 0 rgba(0, 0, 0, .1);
    position: absolute;
    z-index: 2;
    bottom: 20px;
    left: 30px;
    opacity: 0.8;
    color: #FFF;
    font-weight: 400;
  }
  .text-bottom-left a {
    color: #FFF;
  }
  .text-bottom-right {
    text-shadow: -1px -1px 0 rgba(0, 0, 0, .1), 1px -1px 0 rgba(0, 0, 0, .1), -1px 1px 0 rgba(0, 0, 0, .1), 1px 1px 0 rgba(0, 0, 0, .1);
    position: absolute;
    z-index: 2;
    bottom: 20px;
    right: 30px;
    opacity: 0.8;
    color: #FFF;
    font-weight: 400;
    font-size: 10px;
    opacity: 0.4;
  }
  .text-bottom-right a {
    color: #FFF;
  }
  a.recover-pw {
  }
  @media screen and (max-width: 840px) {
    .d-c.left {
      display: none
    }
    .d-c.r {
      padding-right: 0;
      width: 100%;
    }
    .login-tab, .logo {
      margin: auto;
    }
    .text-bottom-left, .text-bottom-right {
      z-index: 2;
      bottom: 20px;
      left: 0;
      right: 0;
      position: relative;
      display: block;
      width: 100%;
      text-align: center;
      bottom: 0;
      top: 0;
    }
    .text-bottom-right {
      margin-top: 10px;
    }
    .d-t {
      bottom: 0;
      left: 0;
      position: relative;
      right: 0;
      top: 0;
      display: table;
      height: 100%;
      width: 100%;
      -webkit-box-sizing: border-box;
      box-sizing: border-box;
      margin: 30px auto;
    }
  }
  @media screen and (max-width: 470px) {
    .login-tab {
      width: 100%;
      -webkit-border-radius: 0;
      -moz-border-radius: 0;
      border-radius: 0;
    }
    .login-tab.dev {
      width: 100%;
      -webkit-border-radius: 0;
      -moz-border-radius: 0;
      border-radius: 0;
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
    }
    .login-tab ul.options li a {
      font-size: 13px !important;
      padding: 10px;
    }
    .login-tab.dev label {
      font-size: 12px;
      font-weight: normal;
    }
    .login-tab.dev .input-login {
      width: 50%;
      font-size: 12px;
    }
    .login-tab.dev label[for="password2"] {
      clear: left;
    }
    .text-bottom-left {
      display: none;
    }
  }
}
.app-auth-uloc {
  background-color: rgba(102, 153, 255, 0.15);
  z-index: 0;
  min-height: 100vh;
  height 100%;
  .logo-image {
    svg {
      width: 177px;
      height: 48px;
    }
    display inline-block;
  }
  .fundo {
    min-height: 100vh;
    background-color: rgba(102, 153, 255, 0.15);
    z-index: 0;
  }
  .novidades {
    min-height: 100vh;
    background-image: linear-gradient(44deg, #8CB638, #8CB638, #0E867D, #0E867D);
    background-size: 400% 400%;
    -webkit-animation: Gradient 10s ease infinite;
    animation: Gradient 10s ease infinite;
    padding-left: 95px;
    z-index: 0;
    .conteudo {
      .contexto {
        -webkit-transform: translate(0, -50%);
        transform: translate(0, -50%);
        position: absolute;
        top: 50%;
      }
      .bem-vindo {
        font-family: 'Open Sans';
        font-size: 36px;
        font-weight: 100;
        line-height: 1.33;
        text-align: left;
        color: #ffffff;
      }
      .mensagem {
        font-family: 'Open Sans';
        font-size: 48px;
        width: 380px;
        font-weight: bold;
        line-height: 1.33;
        text-align: left;
        color: #ffffff;
      }
    }
  }
  .regiao-login {
    position: absolute;
    height: 486px;
    border-radius: 2px;
    background-color: #ffffff;
    box-shadow: 20px 20px 20px 0 rgba(0, 0, 0, 0.25);
    z-index: 1;
    overflow: hidden;
    transition: height 0.4s ease-in-out;
    padding-top: 40px;
    text-align: center;
    letter-spacing: 0.7px;
  }
  .animated {
    -webkit-animation-duration: 0.7s;
    animation-duration: 0.7s;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both;
  }
  .btn-login {
    padding 14px 0
    i {
      position absolute
      right 10px
    }
  }
  @media screen and (min-width: 992px) {
    .regiao-login {
      width: 490px;
    }
  }
  @media only screen and (min-width: 992px) {
    .regiao-login {
      top: 50%;
      left: 50%;
      -webkit-transform: translate(-450px, -50%);
      transform: translate(-450px, -50%);
    }
  }
  @media screen and (max-width: 992px) {
    .regiao-login {
      display: table-cell;
      vertical-align: middle;
      width: 100%;
      max-width: 490px;
      left: 50%;
      -webkit-transform: translate(-50%, 0);
      transform: translate(-50%, 0);
    }
  }
  @media screen and (max-width: 992px) and (min-height: 600px) {
    .regiao-login {
      margin-top: 95px;
    }
  }
  @media screen and (max-width: 992px) {
    .regiao-login {
      top: 0%;
    }
  }
  @-webkit-keyframes Gradient {
    0% {
      background-position: 0% 50%;
    }
    50% {
      background-position: 100% 50%;
    }
    100% {
      background-position: 0% 50%;
    }
  }
  @keyframes Gradient {
    0% {
      background-position: 0% 50%;
    }
    50% {
      background-position: 100% 50%;
    }
    100% {
      background-position: 0% 50%;
    }
  }
  @media (max-height: 720px) {
    .auth-content {
      .auth-separator, .title {
        display none
      }
      .auth-form {
        padding-top 30px !important
      }
    }
  }
}
